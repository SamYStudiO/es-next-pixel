<theme>

	<formatVersion>4</formatVersion>
	
	<view name="system">
		<image name="console" extra="true">
			<path>/recalbox/share_init/system/.emulationstation/themes/recalbox-next/default/data/console.svg</path>
		</image>
		<image name="console" extra="true" region="eu">
			<path>/recalbox/share_init/system/.emulationstation/themes/recalbox-next/default/data/eu/console.svg</path>
		</image>
		<image name="console" extra="true" region="jp">
			<path>/recalbox/share_init/system/.emulationstation/themes/recalbox-next/default/data/jp/console.svg</path>
		</image>
		<image name="console" extra="true" region="us">
			<path>/recalbox/share_init/system/.emulationstation/themes/recalbox-next/default/data/us/console.svg</path>
		</image>
	</view>
	
	<view name="basic, detailed">

		<image name="background_right" extra="true">
			<path>./../../default/background_gamelist_right.jpg</path>
		</image>

		<image name="controller" extra="true">
			<path>/recalbox/share_init/system/.emulationstation/themes/recalbox-next/default/data/controller.svg</path>
		</image>
		<image name="controller" extra="true" region="eu">
			<path>/recalbox/share_init/system/.emulationstation/themes/recalbox-next/default/data/eu/controller.svg</path>
		</image>
		<image name="controller" extra="true" region="jp">
			<path>/recalbox/share_init/system/.emulationstation/themes/recalbox-next/default/data/jp/controller.svg</path>
		</image>
		<image name="controller" extra="true" region="us">
			<path>/recalbox/share_init/system/.emulationstation/themes/recalbox-next/default/data/us/controller.svg</path>
		</image>

		<image name="background" extra="true">
			<path>./../../default/background_gamelist_left.png</path>
		</image>

	</view>

	<view name="detailed">

		<rating name="md_rating">
			<filledPath>./../../default/icon_filled.svg</filledPath>
			<unfilledPath>./../../default/icon_empty.svg</unfilledPath>
		</rating>

	</view>
	
</theme>
