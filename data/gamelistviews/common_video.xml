<theme>

	<formatVersion>4</formatVersion>
	
	<view name="detailed">

		<image name="md_image">
			<origin>0 0</origin>
			<pos>0 0</pos>
			<maxSize>1 1</maxSize>
			<zIndex>70</zIndex>
		</image>
		
		<video name="md_video">
			<origin>0 0</origin>
			<zIndex>71</zIndex>
		</video>
		
		<image name="shader" extra="true">
			<zIndex>72</zIndex>
			<path>./../../$system/data/shader.png</path>
			<tile>true</tile>
		</image>
		
		<image name="shadow" extra="true">
			<origin>0 0</origin>
			<zIndex>73</zIndex>
			<path>./../../$system/data/shadow.png</path>
		</image>
		
	</view>
	
</theme>